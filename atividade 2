anon=int(input("digite sua data de nascimento\n"))
anoa=int(input("digite o ano atual\n"))

dia=anoa-anon

if dia==0:
    print("esta no ano para vc se alistar")
if dia>18:
    print("ja deveria ter alistado a",dia-18,"anos")
if dia<18:
     print("ainda nao deu a data para se alistar faltam",18-dia,"anos")
