data=int(input("digite sua data de nascimento"))
idade=int(input("digite sua iadade"))

dia=idade-18
if dia<0:
    dia=dia*-1
else:
    dia=dia
if dia==0:
    print("esta no ano para vc se alistar")
elif dia>18:
    print("ja deveria ter alistado a",dia,"anos")
elif dia<18:
    print("ainda nao deu a data para se alistar faltam",dia,"anos")
elif dia<0:
    print("vc nem nasceu")

